{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["es2018", "dom"],
    "module": "ES2022",
    "moduleResolution": "node",
    "types": ["bun-types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "jsxFactory": "elements.createElement"
  }
}
